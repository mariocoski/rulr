{
  "name": "rulr",
  "author": "ryansmith94",
  "main": "src/index.js",
  "version": "0.0.2",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha dist/**/*.test.js",
    "watch-test": "./node_modules/mocha/bin/mocha dist/**/*.test.js --watch",
    "cover": "./node_modules/mocha/bin/mocha dist/**/*.test.js --require blanket --reporter mocha-lcov-reporter > coverage/coverage.lcov",
    "codecov": "./node_modules/codecov/bin/codecov",
    "build": "babel src --out-dir dist --source-maps",
    "build-watch": "babel src --out-dir dist --source-maps --watch"

  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "blanket": "1.1.9",
    "codecov": "^1.0.1",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "config": {
    "blanket": {
      "pattern": [
        "dist"
      ],
      "data-cover-never": [
        "node_modules",
        "dist/**/*.test.js"
      ],
      "data-cover-ecmaVersion": "6"
    }
  }
}
